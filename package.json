{
  "name": "comet-js-sdk",
  "version": "1.0.0",
  "description": "The SDK for usage with a Comet Backup Comet Server",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "tsc && tsc -p tsconfig.cjs.json && sh fixup.sh",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": ["dist/"],
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "SDK",
    "Comet Backup",
    "Comet Server"
  ],
  "author": "Comet Backup",
  "license": "Copyright Comet Licensing LTD",
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "form-data": "^4.0.0"
  }
}
